    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^4.32.1",
    "@tanstack/react-query-devtools": "^4.32.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "dayjs": "^1.11.9",
