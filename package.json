    "react-hook-form": "^7.45.2",
    "react-redux": "^8.1.2",
    "react-toastify": "^9.1.3",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "universal-cookie": "^4.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/babel-preset-css-prop": "^11.11.0",
    "@svgr/webpack": "^8.0.1",
    "@tailwindcss/typography": "^0.5.9",
    "@types/bcrypt": "^5.0.0",
